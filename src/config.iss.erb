; CFInstaller Config File

; OutputDir (Installer Exe file)
#define OutputExeDir "<%= output_exe_dir %>"

; Information of Ruby package
;
;   ex.) "Ruby 1.9.3-p194" (Ruby A.B.C-pDEF)
;     #define RubyVersion "1.9.3"
;     #define RubyPatch "194"
;
#define RubyVersion "<%= ruby_version %>"
#define RubyPatch "<%= ruby_patch %>"

; Ruby Root Path
;
;   ex.) Package name is "Ruby193-cf_0.3.18"
;     #define RubyPath "C:\cf_installer\package\Ruby193-cf_0.3.18"
#define RubyPath "<%= ruby_path %>"

; CF version
;
;   ex.) "CF 0.3.18" (CF X.Y.Z);     #define CFVersion "0.3.18"
;
#define CFVersion "<%= cf_version %>"
